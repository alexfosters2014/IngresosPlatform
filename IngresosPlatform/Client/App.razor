<Router AppAssembly="@typeof(Program).Assembly" PreferExactMatches="@true">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(MainLayoutOperador)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(MainLayoutOperador)">
            <p>Lo siento no hay nada en esta direccion</p>
        </LayoutView>
    </NotFound>
</Router>
